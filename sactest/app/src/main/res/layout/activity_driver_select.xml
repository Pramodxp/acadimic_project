        android:textAppearance="?android:attr/textAppearanceMedium" />

    <ListView
        android:id="@+id/qstioncategory"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="80dp" >

    </ListView>



</FrameLayout>